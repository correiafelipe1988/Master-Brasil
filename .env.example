# Configurações do Supabase
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configurações de Assinatura Eletrônica
# Escolha um provedor: d4sign ou clicksign
VITE_SIGNATURE_PROVIDER=d4sign
VITE_SIGNATURE_API_URL=https://api.d4sign.com.br/v1
VITE_SIGNATURE_API_TOKEN=your_signature_api_token

# URL do webhook para receber status de assinatura
VITE_WEBHOOK_URL=https://your-app.vercel.app/api/webhooks/signature
VITE_WEBHOOK_SECRET=your_webhook_secret

# Configurações de Email
# Escolha um provedor: resend ou sendgrid
VITE_EMAIL_PROVIDER=resend
VITE_EMAIL_API_KEY=your_email_api_key
VITE_FROM_EMAIL=noreply@masterbrasil.com
VITE_FROM_NAME=Master Brasil

# Configurações de Storage
# Bucket do Supabase para armazenar documentos
VITE_STORAGE_BUCKET=client-documents

# Configurações de Desenvolvimento
VITE_APP_ENV=development
VITE_DEBUG_MODE=true

# URLs da aplicação
VITE_APP_URL=http://localhost:8085
VITE_API_URL=http://localhost:8085/api
